<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="view_divide">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.8dp</item>
        <item name="android:background">@color/view_line_unselect</item>
    </style>

    <style name="include_ticket_1_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/cm_dp_42</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="include_ticket_1_start">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginStart">15dp</item>
    </style>

    <style name="include_ticket_end">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginStart">15dp</item>
    </style>

</resources>